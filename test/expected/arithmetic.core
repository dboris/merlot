module 'Merlot.Arithmetic' ['add'/2,
                       'sub'/2,
                       'mul'/2,
                       'div'/2,
                       'neg'/1]
    attributes []

'add'/2 =
    fun (X, Y) ->
      call 'erlang':'+' (X, Y)
'sub'/2 =
    fun (X, Y) ->
      call 'erlang':'-' (X, Y)
'mul'/2 =
    fun (X, Y) ->
      call 'erlang':'*' (X, Y)
'div'/2 =
    fun (X, Y) ->
      call 'erlang':'/' (X, Y)
'neg'/1 =
    fun (X) ->
      call 'erlang':'-' (X)
end
