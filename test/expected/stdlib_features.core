module 'Merlot.Stdlib_features' ['test_tuple'/1,
                       'positive_only'/1,
                       'sum_to'/1,
                       'countdown'/1,
                       'main'/1]
    attributes []

'test_tuple'/1 =
    fun (Pair) ->
      case Pair of 
  <{A, {B, C}}> when 'true' -> call 'erlang':'+' (call 'erlang':'+' (A, B), C) end
'positive_only'/1 =
    fun (X) ->
      do case call 'erlang':'>' (X, 0) of
  <'true'> when 'true' -> 'ok'
  <'false'> when 'true' -> call 'erlang':'error' ('assertion_failed')
end
call 'erlang':'*' (X, 2)
'sum_to'/1 =
    fun (N) ->
      case call 'erlang':'=<' (N, 0) of
  <'true'> when 'true' -> 0
  <'false'> when 'true' -> call 'erlang':'+' (N, apply 'sum_to'/1 (call 'erlang':'-' (N, 1)))
end
'countdown'/1 =
    fun (N) ->
      case call 'erlang':'=<' (N, 0) of
  <'true'> when 'true' -> []
  <'false'> when 'true' -> [N|apply 'countdown'/1 (call 'erlang':'-' (N, 1))]
end
'main'/1 =
    fun (_) ->
      let <Tuple_result> = apply 'test_tuple'/1 ({1, {2, 10}}) in let <Sum> = apply 'sum_to'/1 (5) in let <Cd> = apply 'countdown'/1 (3) in let <Doubled> = apply 'positive_only'/1 (5) in {Tuple_result, Sum, Cd, Doubled}
end
