'add'/2 =
    fun (X, Y) ->
      call 'erlang':'+' (X, Y)
'factorial'/1 =
    fun (N) ->
      letrec 'loop'/2 = fun (Acc, N) -> case call 'erlang':'=<' (N, 1) of
  <'true'> when 'true' -> Acc
  <'false'> when 'true' -> apply 'loop'/2 (call 'erlang':'*' (Acc, N), call 'erlang':'-' (N, 1))
end in apply 'loop'/2 (1, N)
end
